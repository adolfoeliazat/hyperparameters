{
  "name": "hyperparameters",
  "version": "0.22.3",
  "description": "javascript hyperparameters search",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "npm run lint && npm run cover",
    "test:prod": "cross-env BABEL_ENV=production npm run test",
    "test:only": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "test:examples": "node examples/",
    "cover": "istanbul cover _mocha -- --require babel-core/register --require babel-polyfill --recursive",
    "lint": "eslint src test",
    "clean": "rimraf lib dist es",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min && npm run build:es",
    "build:watch": "echo 'build && watch the COMMONJS version of the package - for other version, run specific tasks' && npm run build:commonjs:watch",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:commonjs:watch": "npm run build:commonjs -- --watch",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:es:watch": "npm run build:es -- --watch",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development node_modules/.bin/rollup src/index.js --config --sourcemap --file dist/hyperparameters.js",
    "build:umd:watch": "npm run build:umd -- --watch",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup src/index.js --config --file dist/hyperparameters.min.js",
    "prepare": "npm run clean && npm test && npm run build",
    "pub": "npm run build && npm publish"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atanasster/hyperparameters.git"
  },
  "keywords": [
    "hyperparameters",
    "hyperopt",
    "tensorflow",
    "es6",
    "tfjs",
    "javascript"
  ],
  "contributors": [
    {
      "name": "Atanas Stoyanov",
      "email": "atanasster@gmail.com",
      "url": "https://github.com/atanasster"
    },
    {
      "name": "Martin Stoyanov",
      "email": "martin.a.stoyanov@gmail.com",
      "url": "https://github.com/martin-stoyanov"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/atanasster/hyperparameters/issues"
  },
  "homepage": "https://github.com/atanasster/hyperparameters#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.3.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "chai-snapshot-tests": "^0.6.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^5.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "rollup-watch": "^4.3.1"
  },
  "pre-commit": [
    "test"
  ]
}
